parameter (or unit (or mumav timestamp));
storage (pair address timestamp);
code { DUP ;
       DIP { CDR } ;
       CAR ;
       DUP ;
       IF_LEFT { DROP ;
                 { DIP { DUP } ; SWAP } ;
                 NIL operation ;
                 PAIR }
               { IF_LEFT { RENAME ;
                           { DIP { { DIP { DUP } ; SWAP } } ; SWAP } ;
                           DUP ;
                           CAR ;
                           SENDER ;
                           COMPARE ;
                           NEQ ;
                           IF { PUSH string "invalid sender" ;
                                FAILWITH }
                              { DUP ;
                                CDR ;
                                NOW ;
                                COMPARE ;
                                LT ;
                                IF { PUSH string "haven't reached the lock time" ;
                                     FAILWITH }
                                   { { DIP { DUP } ; SWAP } ;
                                     BALANCE ;
                                     COMPARE ;
                                     LT ;
                                     IF { PUSH string "balance is not enough" ;
                                          FAILWITH }
                                        { DUP ;
                                          NIL operation ;
                                          SENDER ;
                                          CONTRACT unit ;
                                          IF_NONE { PUSH string "invalid sender" ; FAILWITH } {} ;
                                          RENAME ;
                                          { DIP { { DIP { { DIP { { DIP { DUP } ; SWAP } } ; SWAP } } ; SWAP } } ; SWAP } ;
                                          UNIT ;
                                          TRANSFER_TOKENS ;
                                          CONS ;
                                          PAIR } } } ;
                           DIP { DROP ; DROP } }
                         { RENAME ;
                           { DIP { { DIP { DUP } ; SWAP } } ; SWAP } ;
                           DUP ;
                           CAR ;
                           SENDER ;
                           COMPARE ;
                           NEQ ;
                           IF { PUSH string "invalid sender" ;
                                FAILWITH }
                              { DUP ;
                                CDR ;
                                NOW ;
                                COMPARE ;
                                LT ;
                                IF { PUSH string "haven't reached the locker time" ;
                                     FAILWITH }
                                   { NOW ;
                                     { DIP { { DIP { DUP } ; SWAP } } ; SWAP } ;
                                     COMPARE ;
                                     LT ;
                                     IF { PUSH string "the parameter time should be greater than current time" ;
                                          FAILWITH }
                                        { DUP ;
                                          CAR ;
                                          { DIP { { DIP { DUP } ; SWAP } } ; SWAP } ;
                                          SWAP ;
                                          PAIR ;
                                          NIL operation ;
                                          PAIR } } } ;
                           DIP { DROP ; DROP } } } ;
       DIP { DROP ; DROP } }